   <marc:datafield tag="245" ind1="1" ind2="0">
      <marc:subfield code="a">Factors influencing genetic evaluations of linebred Hereford cattle in diverse environments</marc:subfield>
      <marc:subfield code="h">[electronic resource].</marc:subfield>
   </marc:datafield>
  
